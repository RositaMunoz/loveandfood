
<h1>Edita la comuna</h1>

<%= form_with(model: @comunas, method: :patch) do |form| %>
    <div class='field'>
	    <%= form.label :nombre %>
	    <%= form.text_field :nombre %>
    </div>

    <div class="actions">
        <%= form.submit %>
    </div>
<% end %>

<br>
<%= button_to "Lista de comunas", { :action => "index" }, :method => :get %>